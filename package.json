{
  "name": "vite-plugin-specifier",
  "version": "1.0.0-alpha.1",
  "description": "Vite plugin to update your ESM or CJS specifiers.",
  "type": "module",
  "main": "dist",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "prettier": "prettier -w src/*.ts",
    "lint": "eslint . src --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "duel",
    "prepack": "npm run build"
  },
  "keywords": [
    "vite-plugin",
    "rollup-plugin",
    "specifiers",
    "esm",
    "cjs",
    "rename",
    "extensions"
  ],
  "files": [
    "dist"
  ],
  "author": "Morgan Ney <morganney@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@knighted/duel": "1.0.0-rc.9",
    "@types/node": "^20.4.8",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "prettier": "^3.0.1",
    "typescript": "^5.2.0-dev.20230805"
  },
  "peerDependencies": {
    "vite": "^4.4.8"
  },
  "dependencies": {
    "@knighted/specifier": "^1.0.0-rc.1"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 90,
    "semi": false,
    "singleQuote": true
  }
}
